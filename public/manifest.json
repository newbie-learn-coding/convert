  "screenshots": [],
  "shortcuts": [
    {
      "name": "Image Converter",
      "short_name": "Images",
      "description": "Convert images between formats",
      "url": "/?mode=image",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Video Converter",
      "short_name": "Video",
      "description": "Convert videos between formats",
      "url": "/?mode=video",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Audio Converter",
      "short_name": "Audio",
      "description": "Convert audio between formats",
      "url": "/?mode=audio",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "protocol_handlers": [],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".webp", ".svg", ".gif", ".bmp", ".tiff"]
      },
      "name": "Convert Image",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "action": "/",
      "accept": {
        "video/*": [".mp4", ".mov", ".avi", ".webm", ".mkv"]
      },
      "name": "Convert Video",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    },
    {
      "action": "/",
      "accept": {
        "audio/*": [".mp3", ".wav", ".flac", ".ogg", ".aac"]
      },
      "name": "Convert Audio",
      "icons": [{ "src": "/icons/icon-96x96.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [{
        "name": "file",
        "accept": ["image/*", "video/*", "audio/*", ".pdf", ".doc", ".docx", ".zip"]
      }]
    }
  }
}
