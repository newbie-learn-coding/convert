{
  "name": "convert",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "pseo:build": "node scripts/build-pseo.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docker": "bun run docker:build && bun run docker:up",
    "docker:build": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml build --build-arg VITE_COMMIT_SHA=$(git rev-parse HEAD)",
    "docker:up": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.override.yml up -d",
    "cf:deploy": "bash scripts/deploy.sh production",
    "cf:deploy:staging": "bash scripts/deploy.sh staging",
    "cf:deploy:dry-run": "bash scripts/deploy.sh production --dry-run",
    "cf:logs:check": "bash scripts/cf-log-check.sh production --base-url $CF_DEPLOY_BASE_URL",
    "cf:rollback": "bash scripts/cf-rollback.sh production",
    "check:seo-policy": "node scripts/check-seo-domain-policy.mjs",
    "check:integrity": "node scripts/check-critical-files.mjs",
    "check:cf-assets": "node scripts/check-cloudflare-asset-sizes.mjs",
    "check:performance-budgets": "node scripts/check-performance-budgets.mjs",
    "test:unit": "bun test src/*.unit.test.ts test/TraversionGraph.test.ts test/handlerErrorNormalization.test.ts test/logging.test.ts",
    "test:ops-hardening": "bun test test/seoDomainPolicy.test.ts test/criticalFileIntegrity.test.ts test/cloudflareWorkerHardening.test.ts test/deployScriptHardening.test.ts",
    "test:browser": "bun test test/browserSmoke.test.ts",
    "test:browser:full": "bun test test/commonFormats.test.ts",
    "test:integration": "bun test test/integration/index.test.ts",
    "test:integration:flows": "bun test test/integration/conversion-flows.test.ts",
    "test:integration:errors": "bun test test/integration/error-handling.test.ts",
    "test:integration:ui": "bun test test/integration/ui-interactions.test.ts",
    "test:integration:a11y": "bun test test/integration/accessibility.test.ts",
    "test:integration:large": "bun test test/integration/large-files.test.ts",
    "test:integration:compat": "bun test test/integration/browser-compatibility.test.ts",
    "test:all": "bun run test:unit && bun run test:integration",
    "validate:production-readiness": "bash scripts/validate-production-readiness.sh",
    "validate:safe": "bash scripts/validate-safe.sh",
    "validate:ci": "VALIDATE_INCLUDE_BUILD=1 bun run validate:safe"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "puppeteer": "^24.36.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^6.0.5"
  },
  "dependencies": {
    "@bjorn3/browser_wasi_shim": "^0.4.2",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@flo-audio/reflo": "^0.1.2",
    "@imagemagick/magick-wasm": "^0.0.37",
    "@sqlite.org/sqlite-wasm": "^3.51.2-build6",
    "@std/yaml": "npm:@jsr/std__yaml",
    "@types/bun": "^1.3.9",
    "@types/meyda": "^5.3.0",
    "@types/pako": "^2.0.4",
    "@types/three": "^0.182.0",
    "imagetracer": "^0.2.2",
    "jszip": "^3.10.1",
    "meyda": "^5.6.3",
    "mime": "^4.1.0",
    "nbtify": "^2.2.0",
    "pako": "^2.1.0",
    "pdftoimg-js": "^0.2.5",
    "pe-library": "^2.0.1",
    "three": "^0.182.0",
    "three-bvh-csg": "^0.0.17",
    "three-mesh-bvh": "^0.9.8",
    "ts-flp": "^1.0.3",
    "vite-plugin-static-copy": "^3.1.6",
    "wavefile": "^11.0.0"
  }
}
