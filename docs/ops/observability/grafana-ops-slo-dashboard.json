{
  "title": "converttoit /_ops SLO",
  "uid": "converttoit-ops-slo",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "tags": ["converttoit", "ops", "slo"],
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Availability (5m)",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\",status_code=~\"2..|3..\"}[5m])) / clamp_min(sum(rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m])), 1)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red" },
              { "color": "yellow", "value": 0.995 },
              { "color": "green", "value": 0.999 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Error Rate (5m)",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\",status_code=~\"5..\"}[5m])) / clamp_min(sum(rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m])), 1)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 0.005 },
              { "color": "red", "value": 0.01 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "p95 Latency (5m)",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(converttoit_ops_request_duration_seconds_bucket{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m])))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 0.2 },
              { "color": "red", "value": 0.3 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Rate Limited Ratio (5m)",
      "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(converttoit_ops_rejections_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\",reason=\"rate_limited\"}[5m])) / clamp_min(sum(rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m])), 1)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.02 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Request Rate by Status Class",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status_class) (rate(converttoit_ops_requests_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Rejections by Reason",
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (reason) (rate(converttoit_ops_rejections_total{endpoint=~\"/_ops/(health|version|metrics|log-ping)\"}[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ]
}
